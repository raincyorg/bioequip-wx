/* pages/my/index.wxss */
.container {
  background-color: #F2f2f2;
}
.userinfo {
  display: flex;
  width:100%;
  flex-direction: column;
  padding: 50rpx 0;
  align-items: center;
  background: #F2f2f2;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #333;
  font-family: '微软雅黑';
}